  </servlet>
  <servlet>
  	<servlet-name>PurchaseGuide</servlet-name>
  	<display-name>PurchaseGuide</display-name>
  	<description></description>
  	<servlet-class>servlet.PurchaseGuide</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Test</servlet-name>
  	<url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ToolsGuide</servlet-name>
  	<url-pattern>/guide/tools</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PurchaseGuide</servlet-name>
  	<url-pattern>/guide/purchase</url-pattern>
